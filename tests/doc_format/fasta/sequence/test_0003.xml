<multi-test>

    <!-- in file exist 3 sequences with difrent Alphabets; 1- NUCL DNA basic 2- NUCL DNA extended 3- RAW -->
    <!-- 1st  sequence's alphabet id must be  NUCL_DNA_DEFAULT_ALPHABET-->

    <load-document index="doc" url="fasta/base_ext_nucl_all_symb.fa" io="local_file" format="fasta"/>

    <find-object-by-name index="obj" doc="doc" name="SEQUENCE_1" type="OT_SEQUENCE"/>

    <check-sequence-alphabet-by-id obj="obj" value="NUCL_DNA_DEFAULT_ALPHABET"/>

</multi-test>
