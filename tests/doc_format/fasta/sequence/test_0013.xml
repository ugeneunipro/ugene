<multi-test>
    <!-- test all allowed symbols in sequence (AMINO DEFAULT ALPHABET)-->
    <!--ACDEFGH  IKLMN  PQRST  VWXY-->
    <!--all symbols except BJOUZ-->

    <load-document index="doc" url="fasta/all_alphabet.fa" io="local_file" format="fasta"/>

    <find-object-by-name index="obj1" doc="doc" name="SEQUENCE_2" type="OT_SEQUENCE"/>
    <check-sequence-size obj="obj1" value="40"/>
    <check-sequence-alphabet-by-id obj="obj1" value="AMINO_DEFAULT_ALPHABET"/>
    <check-sequence-part obj="obj1" value="HHIKLMN" seqstart="8"/>

</multi-test>
