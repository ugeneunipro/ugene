<multi-test>

    <!-- try to check alphabet  in multy sequence (must be NUCL_DNA_DEFAULT_ALPHABET) -->

    <load-document index="doc" url="nexus/dna.nex" io="local_file" format="nexus"/>
    <find-object-by-name index="obj" doc="doc" name="dna" type="OT_MSA"/>
    <check-Msequence-alphabet-by-id obj="obj" value="NUCL_DNA_DEFAULT_ALPHABET"/>

</multi-test>
