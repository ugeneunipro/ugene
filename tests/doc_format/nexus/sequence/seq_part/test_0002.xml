<multi-test>

    <!-- try to find seqpart in dna multy sequence in the middle -->

    <load-document index="doc" url="nexus/dna.nex" io="local_file" format="nexus"/>
    <find-object-by-name index="obj" doc="doc" name="dna" type="OT_MSA"/>

    <check-Msequence-part obj="obj" value="TTACGAATTCTATACATAAT" seqstart="240" seqname="Cow"/>
    <check-Msequence-part obj="obj" value="CTACGCATCCTGTACCTTAT" seqstart="240" seqname="Carp"/>
    <check-Msequence-part obj="obj" value="CTCCAAATCCTCTACATAAT" seqstart="240" seqname="Chicken"/>
    <check-Msequence-part obj="obj" value="CTACGCATCCTTTACATAAC" seqstart="240" seqname="Human"/>
    <check-Msequence-part obj="obj" value="CTACGAATTCTATATCTTAT" seqstart="240" seqname="Loach"/>
    <check-Msequence-part obj="obj" value="CTACGCATTCTATATATAAT" seqstart="240" seqname="Mouse"/>
    <check-Msequence-part obj="obj" value="CTACGAATTCTATACATAAT" seqstart="240" seqname="Rat"/>
    <check-Msequence-part obj="obj" value="TTACGAATCCTCTACATAAT" seqstart="240" seqname="Seal"/>
    <check-Msequence-part obj="obj" value="TTACGGATCCTTTACATAAT" seqstart="240" seqname="Whale"/>
    <check-Msequence-part obj="obj" value="CTTCGTATCCTATATTTAAT" seqstart="240" seqname="Frog"/>

</multi-test>
