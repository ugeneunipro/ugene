<multi-test>

    <!-- try to find seqpart in dna multy sequence in the end -->

    <load-document index="doc" url="nexus/dna.nex" io="local_file" format="nexus"/>
    <find-object-by-name index="obj" doc="doc" name="dna" type="OT_MSA"/>

    <check-Msequence-part obj="obj" value="------------TAA" seqstart="690" seqname="Cow"/>
    <check-Msequence-part obj="obj" value="TCGCTAGGAAGCTAA" seqstart="690" seqname="Carp"/>
    <check-Msequence-part obj="obj" value="---CTGTCATCTTAA" seqstart="690" seqname="Chicken"/>
    <check-Msequence-part obj="obj" value="GTATTTACCCTATAG" seqstart="690" seqname="Human"/>
    <check-Msequence-part obj="obj" value="TCACTAGGAAGCTAA" seqstart="690" seqname="Loach"/>
    <check-Msequence-part obj="obj" value="------------TAA" seqstart="690" seqname="Mouse"/>
    <check-Msequence-part obj="obj" value="------------TAA" seqstart="690" seqname="Rat"/>
    <check-Msequence-part obj="obj" value="------------TAA" seqstart="690" seqname="Seal"/>
    <check-Msequence-part obj="obj" value="------------TAA" seqstart="690" seqname="Whale"/>
    <check-Msequence-part obj="obj" value="TCACTA------AGA" seqstart="690" seqname="Frog"/>

</multi-test>
