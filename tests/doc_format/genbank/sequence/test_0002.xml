<multi-test>

    <!-- in file exist 3 sequences with difrent Alphabets; 1- NUCL DNA basic 2- NUCL DNA extended 3- RAW -->
    <!--test all sequences size -->

    <load-document index="doc" url="genbank/base_ext_nucl_all_symb.gen" io="local_file" format="genbank"/>

    <find-object-by-name index="obj" doc="doc" name="A" type="OT_SEQUENCE"/>
    <check-sequence-size obj="obj" value="70"/>

    <find-object-by-name index="obj1" doc="doc" name="B" type="OT_SEQUENCE"/>
    <check-sequence-size obj="obj1" value="70"/>

    <find-object-by-name index="obj2" doc="doc" name="C" type="OT_SEQUENCE"/>
    <check-sequence-size obj="obj2" value="70"/>


</multi-test>
