<multi-test>
    <!-- COI.hmm, COI.fa dna extended alphabet 
	./hmmsearch COI.hmm COI.fa > g_COI_hmm_COI_fa.out -->

    <load-document index="seq" url="hmmer3/search/COI.fa" io="local_file" format="fasta"/>
    <hmm3-search hmm="hmmer3/search/COI.hmm" seqDoc="seq" outputDir="common/hmmsearch/general_search/default_options/nucl_dna_al/COI/"/>
    <hmm3-search-compare trueOut="hmmer3/search/g_COI_hmm_COI_fa.out" actualOut="common/hmmsearch/general_search/default_options/nucl_dna_al/COI/output.txt"/>

</multi-test>
