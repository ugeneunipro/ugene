<multi-test>
    <!-- Input sequence name consists of ascii characters of all types. -->
    <load-document index="inp_doc" url="et/mfold/bad_names/ascii.fa" io="local_file" format="fasta"/>
    <find-object-by-name index="inp" doc="inp_doc" name="!#$%'()*+,-./0:;=>?@A[\]^_`a{|}~" type="OT_SEQUENCE"/>
    <mfold inp="inp" region="1..50"/>
</multi-test>
