<multi-test>
    <run-cmdline
            task="!common_data_dir!cmdline/find-pattern/find-ptrn.uws"
            in="!common_data_dir!fasta/Nucl_dna_ext_al_seq.fa"
            out="!tmp_data_dir!fp_0009.gb"
            ptrn="AGTCGCRXBDGNDYRACWGG"
            support-ambig-bases="true"
            task-remote-machine="!config_file_path!"
    />

    <load-document index="doc1" url="fp_0009.gb" io="local_file" format="genbank" dir="temp"/>
    <load-document index="doc2" url="cmdline/find-pattern/09.gb" io="local_file" format="genbank"/>

    <compare-annotations-num-in-two-objects doc="doc1" value="doc2"/>
    <compare-annotations-locations-in-two-objects doc="doc1" value="doc2"/>
</multi-test>
