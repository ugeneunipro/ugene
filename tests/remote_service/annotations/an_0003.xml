<multi-test>
    <run-cmdline
            task="!common_data_dir!cmdline/annotations/merge.uwl"
            in="!common_data_dir!genbank/70Bp.gen"
            out="!tmp_data_dir!an_0003.gbk"
            merge-with="!common_data_dir!genbank/70Bp_new.gb"
            task-remote-machine="!config_file_path!"
    />

    <load-document index="doc1" url="genbank/70Bp_new.gb" io="local_file" format="genbank"/>
    <load-document index="doc2" url="an_0003.gbk" io="local_file" format="genbank" dir="temp"/>

    <compare-annotations-num-in-two-objects doc="doc1" value="doc2"/>
    <compare-annotations-locations-in-two-objects doc="doc1" value="doc2"/>

</multi-test>
