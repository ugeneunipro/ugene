<multi-test>
    <!-- Checking Smith-Waterman algorithm -->

    <load-document index="searchSeqDoc" url="smith_waterman2/simple/04/search.txt" io="local_file" format="fasta"/>
    <find-object-by-name index="searchSeq" doc="searchSeqDoc" name="S" type="OT_SEQUENCE"/>

    <load-document index="patternSeqDoc" url="smith_waterman2/simple/04/query.txt" io="local_file" format="fasta"/>
    <find-object-by-name index="patternSeq" doc="patternSeqDoc" name="Q" type="OT_SEQUENCE"/>


    <plugin_sw-algorithm
            subst_f="smith_waterman2/blosum62.txt"
            seq_f="searchSeq"
            pattern_f="patternSeq"
            g_o="-1"
            g_e="-1"
            percent_of_score="38"
            expected_res="8,7..9**8,8..10**8,9..11**8,10..12**8,10..13"
    />

</multi-test>
