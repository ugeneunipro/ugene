<multi-test>
    <!-- Checking Smith-Waterman algorithm -->

    <load-document index="searchSeqDoc" url="smith_waterman2/multi/01/search.txt" io="local_file" format="fasta"/>
    <find-object-by-name index="searchSeq" doc="searchSeqDoc" name="S" type="OT_SEQUENCE"/>

    <load-document index="patternSeqDoc" url="smith_waterman2/multi/01/query.txt" io="local_file" format="fasta"/>
    <find-object-by-name index="patternSeq" doc="patternSeqDoc" name="Q" type="OT_SEQUENCE"/>


    <plugin_sw-algorithm
            subst_f="smith_waterman2/blosum62.txt"
            seq_f="searchSeq"
            pattern_f="patternSeq"
            g_o="-1"
            g_e="-1"
            percent_of_score="49"
            expected_res="63,31..55**62,31..53**62,31..54"
    />

</multi-test>
