<!-- Check the consensus calculation: simple extended algorithm, threshold = 50% -->
<multi-test>
    <run-cmdline
            task="extract_consensus_sequence"
            in="!common_data_dir!scenarios/msa/simple_extended_consensus7.aln"
            algorithm="Simple extended"
            threshold="87"
            out="!tmp_data_dir!simple_extended_consensus_0007.fa"
    />

    <load-document index="actual" url="simple_extended_consensus_0007.fa" io="local_file" format="fasta" dir="temp"/>
    <load-document index="expected" url="scenarios/consensus/simple_extended_87.fa" io="local_file" format="fasta"/>
    <compare-sequences-in-two-objects doc="actual" value="expected"/>

</multi-test>
