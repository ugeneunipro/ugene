<?xml version="1.0" ?>
<multi-test>
	<load-document index="doc" url="primer3/primer3_xml/overhang/Overhang09.fa" io="local_file" format="fasta"/>
	<find-object-by-name index="seq" doc="doc" name="Overhang09" type="OT_SEQUENCE"/>
	<plugin_primer_3 sequence="seq">
		<plugin_primer_3_in 
			PRIMER_TASK="check_primers" 
			PRIMER_PICK_ANYWAY="1" 
			PRIMER_NUM_RETURN="1" 
			PRIMER_EXPLAIN_FLAG="1" 
			PRIMER_MAX_NS_ACCEPTED="100" 
			PRIMER_MAX_SIZE="35" 
			PRIMER_MIN_SIZE="14" 
			PRIMER_PRODUCT_SIZE_RANGE="70-1000" 
			PRIMER_PICK_LEFT_PRIMER="1" 
			PRIMER_PICK_RIGHT_PRIMER="1" 
			PRIMER_PICK_INTERNAL_OLIGO="0" 
			PRIMER_SECONDARY_STRUCTURE_ALIGNMENT="0" 
			PRIMER_LIBERAL_BASE="0" 
			SEQUENCE_PRIMER="TGCTTTCAATCATGTGGGTGA" 
			SEQUENCE_PRIMER_REVCOMP="ATGAGAGCAAGTTGGCGAAG" 
			SEQUENCE_OVERHANG_LEFT="Z" 
			SEQUENCE_OVERHANG_RIGHT="ATCAG" 
			P3_COMMENT="non-ACGT base in left overhang with PRIMER_LIBERAL_BASE"/>
		<plugin_primer_3_out 
			PRIMER_ERROR="Unrecognized base in left overhang"/>
	</plugin_primer_3>
</multi-test>
