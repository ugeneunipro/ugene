<?xml version="1.0" ?>
<multi-test>
	<load-document index="doc" url="primer3/primer3_xml/primer1/other_bad_target.fa" io="local_file" format="fasta"/>
	<find-object-by-name index="seq" doc="doc" name="other_bad_target" type="OT_SEQUENCE"/>
	<plugin_primer_3 sequence="seq">
		<plugin_primer_3_in 
			PRIMER_FIRST_BASE_INDEX="1" 
			PRIMER_MAX_SELF_ANY="12" 
			PRIMER_PAIR_MAX_COMPL_ANY="12" 
			PRIMER_MAX_POLY_X="100" 
			PRIMER_INTERNAL_MAX_POLY_X="100" 
			PRIMER_MAX_SELF_END="8" 
			PRIMER_PAIR_MAX_COMPL_END="8" 
			SEQUENCE_TARGET="101,1000"/>
		<plugin_primer_3_out 
			PRIMER_ERROR="TARGET beyond end of sequence"/>
	</plugin_primer_3>
</multi-test>
