<?xml version="1.0" ?>
<multi-test>
	<load-document index="doc" url="primer3/primer3_xml/primer1/SP03-40.txt.fa" io="local_file" format="fasta"/>
	<find-object-by-name index="seq" doc="doc" name="SP03-40.txt" type="OT_SEQUENCE"/>
	<plugin_primer_3 sequence="seq">
		<plugin_primer_3_in 
			PRIMER_FIRST_BASE_INDEX="1" 
			PRIMER_MAX_SELF_ANY="12" 
			PRIMER_PAIR_MAX_COMPL_ANY="12" 
			PRIMER_MAX_POLY_X="100" 
			PRIMER_INTERNAL_MAX_POLY_X="100" 
			PRIMER_MAX_SELF_END="8" 
			PRIMER_PAIR_MAX_COMPL_END="8"/>
		<plugin_primer_3_out 
			PRIMER_ERROR="SEQUENCE_INCLUDED_REGION length &lt; min PRIMER_PRODUCT_SIZE_RANGE"/>
	</plugin_primer_3>
</multi-test>
