                 in="!common_data_dir!cmdline/custom-script-worker-functions/fastq_quality_trimmer/fastq1.fastq"
                 out_dir="!tmp_data_dir!"
                 out_name="fastq_quality_trimmer_res03.fastq"
                 qual="20"
                 len="12"/>
