<multi-test>
    <add-shared-db
            url="ugene-quad-ubuntu"
            port="3306"
            db_name="ugene_gui_test_win"
            user="gui_tester"
            password="123"
            custom-db-name="Test Shared DB"
    />

    <run-cmdline
            task="!common_data_dir!cmdline/wd-shared-db/read_from_shared_db_by_accession.uwl"
            out="!tmp_data_dir!wd_shared_db_0001.fa"
    />

    <load-document index="actual" url="wd_shared_db_0001.fa" io="local_file" format="fasta" dir="temp"/>
    <load-document index="expected" url="cmdline/wd-shared-db/read_from_shared_db_by_accession.fa" io="local_file" format="fasta"/>
    <compare-sequences-in-two-objects doc="actual" value="expected"/>

</multi-test>
