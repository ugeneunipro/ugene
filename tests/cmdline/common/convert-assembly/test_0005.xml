            task="!common_data_dir!cmdline/read-write/read_assembly_write_assembly.uwl"
            in="!common_data_dir!bam/small.bam.sorted.bam"
            out="!tmp_data_dir!convert_assembly_0005.sam"
            out_format="sam"
    />
