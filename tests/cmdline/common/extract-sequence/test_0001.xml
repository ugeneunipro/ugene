<multi-test>
    <run-cmdline
            task="extract-sequence"
            in="!common_data_dir!genbank/murine_sarcoma.gb"
            out="!tmp_data_dir!extract_seq0001_tmp.fa"
            annotation-names-file="!common_data_dir!cmdline/extract-sequence/names_1.txt"
            accept-or-filter="true"
            format="genbank"
    />

    <load-document index="doc" url="extract_seq0001_tmp.fa" io="local_file" format="fasta" dir="temp"/>

    <check-num-objects doc="doc" value="4"/>

    <check-document-object-types doc="doc" value="OT_SEQUENCE,OT_SEQUENCE,OT_SEQUENCE,OT_SEQUENCE"/>


</multi-test>
