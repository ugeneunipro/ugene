<multi-test>
    <run-cmdline task="sitecon-search"
                 in="!common_data_dir!/Sitecon/Algorithm/human_T1.fa"
                 out="!tmp_data_dir!human_T1_actual4.gb"
                 inmodel="!common_data_dir!/Sitecon/Algorithm/siteconBuilds/e2f_30_w2.sitecon"
                 strand="1"
                 min-score="79"
                 min-err1="0"
                 max-err2="1"
    />
    <load-document index="doc2" url="human_T1_actual4.gb" io="local_file" format="genbank" dir="temp"/>
    <load-document index="doc1" url="/Sitecon/Algorithm/human_T1_4.gb" io="local_file" format="genbank"/>

    <compare-annotations-num-in-two-objects doc="doc1" value="doc2"/>
    <compare-annotations-locations-in-two-objects doc="doc1" value="doc2"/>
</multi-test>