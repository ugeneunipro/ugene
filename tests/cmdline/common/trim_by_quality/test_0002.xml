<!-- Check Sanger Phred+33 encoding. -->
<multi-test>

    <run-cmdline
            task="!common_data_dir!cmdline/trim_by_quality/trim-by-qual.uwl"
            in="!common_data_dir!fastq/BT474_short.fastq"
            out-dir="!tmp_data_dir!"
            out="test_0002.fastq"
    />

    <load-document index="actual" url="test_0002.fastq" io="local_file" format="fastq" dir="temp"/>
    <load-document index="expected" url="cmdline/trim_by_quality/BT474_short.fastq.trimmed.fastq" io="local_file" format="fastq"/>
    <compare-sequences-in-two-objects doc="actual" value="expected"/>

</multi-test>
