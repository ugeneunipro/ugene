<multi-test>

    <run-cmdline
            task="!common_data_dir!cmdline/consensus/consensus.uwl"
            in="!common_data_dir!ugenedb/example-alignment.ugenedb"
            out="!tmp_data_dir!consensus_0005.fa"
            algo="Default"
            gaps="false"
    />

    <load-document index="actual" url="consensus_0005.fa" io="local_file" format="fasta" dir="temp"/>
    <load-document index="expected" url="cmdline/consensus/example-alignment_consensus_default_nogaps.fa" io="local_file" format="fasta"/>
    <compare-sequences-in-two-objects doc="actual" value="expected"/>

</multi-test>
